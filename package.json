{
  "name": "vue-recipes",
  "version": "0.1.2",
  "description": "A tiny utility to make type-safe variant Vue components from your vanilla-extract recipes.",
  "keywords": [
    "css",
    "css-in-js",
    "stylesheet",
    "vue"
  ],
  "homepage": "https://github.com/wobsoriano/vue-recipes#readme",
  "bugs": {
    "url": "https://github.com/wobsoriano/vue-recipes/issues"
  },
  "license": "MIT",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/vue-recipes.git"
  },
  "funding": "https://github.com/sponsors/wobsoriano",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vue": ">=3.2.0"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.9.1",
    "@vanilla-extract/recipes": "^0.2.5",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.21.1",
    "@babel/types": "^7.17.0",
    "@types/node": "^17.0.29",
    "bumpp": "^7.1.1",
    "eslint": "^8.14.0",
    "esno": "^0.14.1",
    "pnpm": "^6.32.11",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.4",
    "vitest": "^0.10.0"
  }
}
